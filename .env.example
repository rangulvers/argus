# Argus Environment Variables
# Copy this file to .env and customize

# Database
DATABASE_URL=sqlite:///./data/argus.db

# Web Server
WEB_HOST=0.0.0.0
WEB_PORT=8080

# Authentication (optional)
ENABLE_AUTH=false
AUTH_USERNAME=admin
AUTH_PASSWORD=changeme

# Network Scanning
SUBNET=192.168.1.0/24
SCAN_PROFILE=normal
PORT_RANGE=1-1000

# Logging
LOG_LEVEL=INFO
